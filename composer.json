{
    "name": "cwoskoski/oauth-server",
    "description": "OAuth2 Server for Hyperf Framework",
    "license": "MIT",
    "version": "1.4.0",
    "authors": [
        {
            "name": "Abdul Hafidz Anshari",
            "email": "aditans88@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "hyperf/database": "~3.1.0",
        "hyperf/db-connection": "~3.1.0",
        "league/oauth2-server": "^9.0",
        "nesbot/carbon": "^2.0",
        "phpseclib/phpseclib": "^3.0"
    },
    "require-dev": {
        "hyperf/command": "^3.1@dev",
        "hyperf/config": "^3.1@dev",
        "hyperf/http-server": "^3.1@dev",
        "hyperf/utils": "^3.1@dev"
    },
    "autoload": {
        "psr-4": {
            "OAuthServer\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.4-dev"
        },
        "hyperf": {
            "config": "OAuthServer\\ConfigProvider"
        }
    }
